{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/lib/signalr",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "jquery@3.5.1",
      "destination": "wwwroot/JS/lib/jquery/",
      "files": [
        "jquery.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "knockout@3.5.1",
      "destination": "wwwroot/JS/lib/knockout/",
      "files": [
        "knockout-latest.min.js"
      ]
    }
  ]
}